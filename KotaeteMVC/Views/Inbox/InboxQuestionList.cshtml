@using Resources
@model List<KotaeteMVC.Models.QuestionDetailAnswerViewModel>

@if (Model.Count > 0)
{

    foreach (var question in Model)
    {
        Html.RenderPartial("InboxQuestion", question);
    }
}
else
{
    <div class="jumbotron">
        <h3>@InboxStrings.NoQuestionsTitle</h3>
        <p>@InboxStrings.NoQuestions<span class="glyphicon glyphicon-heart"></span></p>
    </div>
}
