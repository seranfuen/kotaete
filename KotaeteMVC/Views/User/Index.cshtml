@model KotaeteMVC.Models.ProfileQuestionViewModel

@{
    Layout = "~/Views/Shared/_ProfileContentLayout.cshtml";
    ViewBag.Title = Model.Profile.ScreenName;
}


@section Profile {

    @Html.Partial("Profile", Model.Profile)
}

@if (HttpContext.Current.User.Identity.IsAuthenticated && Model.Profile.IsOwnProfile == false)
{
    Html.RenderPartial("Question", Model.QuestionDetail);
}