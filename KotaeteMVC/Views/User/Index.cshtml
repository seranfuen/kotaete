@model KotaeteMVC.Models.ProfileQuestionViewModel

@{
    Layout = "~/Views/Shared/_ProfileContentLayout.cshtml";
    ViewBag.Title = Model.ScreenName;
}


@section Profile {
    @Html.Partial("Profile", Model)
}

@if (HttpContext.Current.User.Identity.IsAuthenticated && Model.IsOwnProfile == false)
{
    Html.RenderPartial("Question", Model);
}